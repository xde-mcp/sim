{
  "name": "simstudio-ts-sdk",
  "version": "0.1.2",
  "description": "Sim SDK - Execute workflows programmatically",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit",
