  },
  "devDependencies": {
    "@sim/tsconfig": "workspace:*",
    "@types/node": "^20.5.1",
    "@vitest/coverage-v8": "^3.0.8",
    "typescript": "^5.7.3",
    "vitest": "^3.0.8"
  },
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/simstudioai/sim.git",
    "directory": "packages/ts-sdk"
  },
  "homepage": "https://sim.ai",
  "bugs": {
    "url": "https://github.com/simstudioai/sim/issues"
